{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Markdown",
  "scopeName": "text.html.markdown",
  "patterns": [
    { "include": "#frontmatter" },
    { "include": "#block" }
  ],
  "repository": {
    "frontmatter": {
      "name": "markup.frontmatter.markdown",
      "begin": "\\A(-{3})\\s*$",
      "end": "^(-{3})\\s*$",
      "beginCaptures": { "1": { "name": "punctuation.definition.frontmatter.markdown" } },
      "endCaptures": { "1": { "name": "punctuation.definition.frontmatter.markdown" } },
      "patterns": [{ "include": "source.yaml" }]
    },
    "block": {
      "patterns": [
        { "include": "#comment" },
        { "include": "#heading-setext" },
        { "include": "#heading-atx" },
        { "include": "#blockquote" },
        { "include": "#horizontal-rule" },
        { "include": "#fenced-code" },
        { "include": "#table" },
        { "include": "#list" },
        { "include": "#definition-list" },
        { "include": "#footnote-definition" },
        { "include": "#paragraph" }
      ]
    },
    "inline": {
      "patterns": [
        { "include": "#escape" },
        { "include": "#inline-code" },
        { "include": "#math-inline" },
        { "include": "#image" },
        { "include": "#link" },
        { "include": "#autolink" },
        { "include": "#footnote-reference" },
        { "include": "#bold-italic" },
        { "include": "#strikethrough" },
        { "include": "#highlight" },
        { "include": "#subscript" },
        { "include": "#superscript" },
        { "include": "#emoji" },
        { "include": "#html-inline" }
      ]
    },
    "comment": {
      "name": "comment.block.html.markdown",
      "begin": "<!--",
      "end": "-->",
      "captures": { "0": { "name": "punctuation.definition.comment.html" } }
    },
    "heading-atx": {
      "patterns": [
        {
          "name": "heading.6.markdown markup.heading.6.markdown",
          "match": "^(######)[ \\t]+(.+?)[ \\t]*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.markdown" },
            "2": { "name": "entity.name.section.markdown", "patterns": [{ "include": "#inline" }] }
          }
        },
        {
          "name": "heading.5.markdown markup.heading.5.markdown",
          "match": "^(#####)[ \\t]+(.+?)[ \\t]*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.markdown" },
            "2": { "name": "entity.name.section.markdown", "patterns": [{ "include": "#inline" }] }
          }
        },
        {
          "name": "heading.4.markdown markup.heading.4.markdown",
          "match": "^(####)[ \\t]+(.+?)[ \\t]*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.markdown" },
            "2": { "name": "entity.name.section.markdown", "patterns": [{ "include": "#inline" }] }
          }
        },
        {
          "name": "heading.3.markdown markup.heading.3.markdown",
          "match": "^(###)[ \\t]+(.+?)[ \\t]*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.markdown" },
            "2": { "name": "entity.name.section.markdown", "patterns": [{ "include": "#inline" }] }
          }
        },
        {
          "name": "heading.2.markdown markup.heading.2.markdown",
          "match": "^(##)[ \\t]+(.+?)[ \\t]*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.markdown" },
            "2": { "name": "entity.name.section.markdown", "patterns": [{ "include": "#inline" }] }
          }
        },
        {
          "name": "heading.1.markdown markup.heading.1.markdown",
          "match": "^(#)[ \\t]+(.+?)[ \\t]*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.markdown" },
            "2": { "name": "entity.name.section.markdown", "patterns": [{ "include": "#inline" }] }
          }
        }
      ]
    },
    "heading-setext": {
      "patterns": [
        {
          "name": "markup.heading.1.setext.markdown",
          "match": "^(.+)\\n(=+)\\s*$",
          "captures": {
            "1": {
              "name": "entity.name.section.markdown",
              "patterns": [{ "include": "#inline" }]
            },
            "2": { "name": "punctuation.definition.heading.setext.markdown" }
          }
        },
        {
          "name": "markup.heading.2.setext.markdown",
          "match": "^(.+)\\n(-+)\\s*$",
          "captures": {
            "1": {
              "name": "entity.name.section.markdown",
              "patterns": [{ "include": "#inline" }]
            },
            "2": { "name": "punctuation.definition.heading.setext.markdown" }
          }
        }
      ]
    },
    "blockquote": {
      "name": "markup.quote.markdown",
      "begin": "^\\s*(>)\\s?",
      "end": "^(?=\\s*$|\\s*[^>])",
      "beginCaptures": { "1": { "name": "punctuation.definition.quote.begin.markdown" } },
      "patterns": [
        { "include": "#blockquote" },
        { "include": "#inline" }
      ]
    },
    "horizontal-rule": {
      "name": "meta.separator.markdown",
      "match": "^\\s*([*\\-_])\\s*\\1\\s*\\1(\\s*\\1)*\\s*$"
    },
    "fenced-code": {
      "patterns": [
        { "include": "#fenced-code-javascript" },
        { "include": "#fenced-code-typescript" },
        { "include": "#fenced-code-tsx" },
        { "include": "#fenced-code-jsx" },
        { "include": "#fenced-code-json" },
        { "include": "#fenced-code-yaml" },
        { "include": "#fenced-code-python" },
        { "include": "#fenced-code-ruby" },
        { "include": "#fenced-code-go" },
        { "include": "#fenced-code-rust" },
        { "include": "#fenced-code-shell" },
        { "include": "#fenced-code-html" },
        { "include": "#fenced-code-css" },
        { "include": "#fenced-code-scss" },
        { "include": "#fenced-code-sql" },
        { "include": "#fenced-code-graphql" },
        { "include": "#fenced-code-dockerfile" },
        { "include": "#fenced-code-diff" },
        { "include": "#fenced-code-c" },
        { "include": "#fenced-code-cpp" },
        { "include": "#fenced-code-java" },
        { "include": "#fenced-code-kotlin" },
        { "include": "#fenced-code-swift" },
        { "include": "#fenced-code-php" },
        { "include": "#fenced-code-lua" },
        { "include": "#fenced-code-r" },
        { "include": "#fenced-code-toml" },
        { "include": "#fenced-code-xml" },
        { "include": "#fenced-code-markdown" },
        { "include": "#fenced-code-generic" }
      ]
    },
    "fenced-code-javascript": {
      "name": "markup.fenced_code.block.markdown",
      "begin": "^(\\s*)(`{3,}|~{3,})\\s*(?i:(javascript|js))\\s*$",
      "end": "^\\1\\2\\s*$",
      "beginCaptures": {
        "2": { "name": "punctuation.definition.markdown" },
        "3": { "name": "fenced_code.block.language.markdown" }
      },
      "endCaptures": { "0": { "name": "punctuation.definition.markdown" } },
      "contentName": "meta.embedded.block.javascript",
      "patterns": [{ "include": "source.js" }]
    },
